          "type": "string"
        },
        "image": {
          "description": "验证码图片",
          "type": "string"
        }
      }
    },
    "model.JSON": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer"
        },
        "data": {
          "type": "object"
        },
        "message": {
          "type": "string"
                }
            }
        }
